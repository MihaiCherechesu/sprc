
# This is a template Makefile generated by rpcgen

# Parameters

CLIENT = api_client
SERVER = api_server

SOURCES_CLNT.cpp = 
SOURCES_CLNT.hpp = 
SOURCES_SVC.cpp = 
SOURCES_SVC.hpp = 
SOURCES.x = api.x

TARGETS_SVC.cpp = api_svc.cpp api_server.cpp api_xdr.cpp 
TARGETS_CLNT.cpp = api_clnt.cpp api_client.cpp api_xdr.cpp 
TARGETS = api.hpp api_xdr.cpp api_clnt.cpp api_svc.cpp api_client.cpp api_server.cpp

OBJECTS_CLNT = $(SOURCES_CLNT.cpp:%.cpp=%.o) $(TARGETS_CLNT.cpp:%.cpp=%.o)
OBJECTS_SVC = $(SOURCES_SVC.cpp:%.cpp=%.o) $(TARGETS_SVC.cpp:%.cpp=%.o)
# Compiler flags 

CFLAGS += -g 
LDLIBS += -lnsl
RPCGENFLAGS = 

# Targets 

all : $(CLIENT) $(SERVER)

$(TARGETS) : $(SOURCES.x) 
	rpcgen $(RPCGENFLAGS) $(SOURCES.x)

$(OBJECTS_CLNT) : $(SOURCES_CLNT.cpp) $(SOURCES_CLNT.hpp) $(TARGETS_CLNT.cpp) 

$(OBJECTS_SVC) : $(SOURCES_SVC.cpp) $(SOURCES_SVC.hpp) $(TARGETS_SVC.cpp) 

$(CLIENT) : $(OBJECTS_CLNT) 
	$(LINK.cpp) -o $(CLIENT) $(OBJECTS_CLNT) $(LDLIBS) 

$(SERVER) : $(OBJECTS_SVC) 
	$(LINK.cpp) -o $(SERVER) $(OBJECTS_SVC) $(LDLIBS)

 clean:
	 $(RM) core $(TARGETS) $(OBJECTS_CLNT) $(OBJECTS_SVC) $(CLIENT) $(SERVER)

